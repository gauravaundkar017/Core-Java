	public SortByGpaSubjectWise(Map<String, Student> studentList, String fName) {
		this.studentList = studentList;
		this.fName = fName;
	}

	@Override
	public void run() {
		Comparator<Student> comp = (s1,s2)->((Double)s1.getGpa()).compareTo(s2.getGpa());


	}

}
